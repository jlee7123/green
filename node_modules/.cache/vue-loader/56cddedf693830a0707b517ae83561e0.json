field>\r\n          </v-col>\r\n        </v-row>\r\n\r\n\r\n      <v-row>\r\n        <v-col></v-col>\r\n        <v-col cols=\"3\">\r\n          <v-btn\r\n            class=\"mt-2\"\r\n            @click=\"GcsAppBarCreated\"\r\n            elevation=\"1\"\r\n            color=\"#64B5F6\"\r\n            :disabled=\"MOBIUS_CONNECTION_CONNECTED\"\r\n            block\r\n            outlined\r\n            raised\r\n            rounded\r\n            v-on:click=\"ae_form\"\r\n          >\r\n            {{ MOBIUS_CONNECTION_TEXT }}\r\n          </v-btn>\r\n        </v-col>\r\n        <v-col cols=\"3\">\r\n          <v-btn\r\n            class=\"mt-2\"\r\n            @click=\"GcsAppBarReseted\"\r\n            elevation=\"1\"\r\n            color=\"#64B5F6\"\r\n            :disabled=\"MOBIUS_CONNECTION_DISABLED\"\r\n            block\r\n            outlined\r\n            raised\r\n            rounded\r\n            v-on:click=\"ae_form\"\r\n          >\r\n            {{ MOBIUS_DISCONNECTION_TEXT }}\r\n          </v-btn>\r\n        </v-col>\r\n        <v-col></v-col>\r\n      </v-row>\r\n      </v-card>\r\n      \r\n      <br /><br />\r\n\r\n      <v-card class=\"pa-7\" outlined shaped elevation=\"3\">\r\n        <v-row>\r\n        <h5 class=\"cardtitle font-weight-black\">DRONE ID</h5>\r\n        <br>\r\n        <br>\r\n\r\n          <v-col>\r\n            <v-text-field\r\n              class=\"mr-2 mb-n2 pb-3\"\r\n              dense\r\n              filled\r\n              shaped\r\n              ref=\"drone_ID\"\r\n              v-model=\"drone_id\"\r\n              :rules=\"drone_id_rule\"\r\n              label=\"* Drone ID\"\r\n              hint=\"Unique ID of Drone\"\r\n              persistent-hint\r\n              required\r\n            ></v-text-field>\r\n          </v-col>\r\n\r\n          <v-col cols=\"3\">\r\n            <v-btn\r\n              class=\"ma-3\"\r\n              @click=\"IDCheck\"\r\n              elevation=\"1\"\r\n              color=\"#64B5F6\"\r\n              :disabled=\"drone_id === null || drone_id === ''\"\r\n              block\r\n              outlined\r\n              raised\r\n              rounded\r\n            >\r\n              CREATE\r\n            </v-btn>\r\n          </v-col>\r\n        </v-row>\r\n      </v-card>\r\n\r\n      <br /><br />\r\n\r\n      <v-card class=\"pa-7\" outlined shaped elevation=\"3\">\r\n<v-row>\r\n        <h5 class=\"cardtitle font-weight-black\">DRONE Specification</h5>\r\n        <br><br>\r\n        <v-col>\r\n           <label class=\"font-weight-bold mt-n2\">HOST :</label>\r\n          <v-text-field\r\n            class=\"mr-2 mb-n2 pb-3\"\r\n            dense\r\n            filled\r\n            shaped\r\n            ref=\"drone_host\"\r\n            v-model=\"drone_host\"\r\n            :rules=\"drone_host_rule\"\r\n            placeholder=\"125.132.13.63\"\r\n            label=\"* GCS Host\"\r\n            hint=\"e.g.) gcs.iotocean.org\"\r\n            persistent-hint\r\n            required\r\n          ></v-text-field>\r\n        </v-col>\r\n\r\n        <v-col>\r\n          <label class=\"font-weight-bold mt-n2\"> DRONE NAME :</label>\r\n          <v-text-field\r\n            class=\"mr-2 mb-n2 pb-3\"\r\n            dense\r\n            filled\r\n            shaped\r\n            ref=\"drone_Name\"\r\n            v-model=\"drone_name\"\r\n            :rules=\"drone_name_rule\"\r\n            placeholder=\"KETI_UAV\"\r\n            label=\"* Drone Name (e.g. KETI_UAV_1)\"\r\n            required\r\n          ></v-text-field>\r\n        </v-col>\r\n</v-row> \r\n\r\n<v-row>\r\n        <v-col>\r\n          <label class=\"font-weight-bold mt-n2\">GCS :</label>\r\n          <v-text-field\r\n            class=\"mr-2 mb-n2 pb-3\"\r\n            dense\r\n            filled\r\n            shaped\r\n            ref=\"gcs_name\"\r\n            v-model=\"gcs_name\"\r\n            :rules=\"gcs_name_rule\"\r\n            placeholder=\"KETI_MUV\"\r\n            label=\"* GCS Name (e.g. KETI_GCS)\"\r\n            required\r\n          ></v-text-field>\r\n        </v-col>\r\n\r\n        <v-col>\r\n          <label class=\"font-weight-bold mt-n2\">TYPE :</label>\r\n          <v-text-field\r\n            class=\"mr-2 mb-n2\"\r\n            dense\r\n            filled\r\n            shaped\r\n            ref=\"drone_type\"\r\n            v-model=\"drone_type\"\r\n            :rules=\"drone_type_rule\"\r\n            :items=\"['ardupilot', 'px4', 'dji', 'etc']\"\r\n            placeholder=\"pixhawk\"\r\n            label=\"* FC Type (e.g. ardupilot or px4)\"\r\n            hint=\"Type of FC\"\r\n            required\r\n          ></v-text-field>\r\n        </v-col>\r\n</v-row>\r\n\r\n<v-row>\r\n        <v-col>\r\n          <label class=\"font-weight-bold mt-n2\">SYSTEM ID :</label>\r\n          <v-text-field\r\n            class=\"mr-2 mb-n2\"\r\n            dense\r\n            filled\r\n            shaped\r\n            ref=\"system_id\"\r\n            v-model=\"system_id\"\r\n            :rules=\"system_id_rule\"\r\n            label=\"* System ID (e.g. 255)\"\r\n            required\r\n          ></v-text-field>\r\n        </v-col>\r\n\r\n       \r\n\r\n        <v-col>\r\n           <label class=\"font-weight-bold mt-n2\">MAVLink VERSION :</label>\r\n          <v-text-field\r\n            class=\"mr-2 mb-n2\"\r\n            dense\r\n            filled\r\n            shaped\r\n            ref=\"mav_ver\"\r\n            v-model=\"mav_ver\"\r\n            :rules=\"mav_ver_rule\"\r\n            label=\"* Mav_ver (e.g. v1)\"\r\n            required\r\n          ></v-text-field>\r\n        </v-col>\r\n</v-row>\r\n\r\n<v-row>\r\n        <v-col>\r\n          <label class=\"font-weight-bold mt-n2\">UPDATE :</label>\r\n          <v-text-field\r\n            class=\"mr-2 mb-n2\"\r\n            dense\r\n            filled\r\n            shaped\r\n            ref=\"update\"\r\n            v-model=\"update\"\r\n            :rules=\"update_rule\"\r\n            label=\"* Update (e.g. disable)\"\r\n            required\r\n          ></v-text-field>\r\n        </v-col>\r\n\r\n       \r\n\r\n        <v-col>\r\n          <label class=\"font-weight-bold mt-n2\">BOARD VERSION :</label>\r\n          <v-text-field\r\n            class=\"mr-2 mb-n2\"\r\n            dense\r\n            filled\r\n            shaped\r\n            ref=\"board_ver\"\r\n            v-model=\"board_ver\"\r\n            :rules=\"board_ver_rule\"\r\n            label=\"* board version (e.g. Crow-A or Crow-B or Crow-C)\"\r\n            required\r\n          ></v-text-field>\r\n        </v-col>\r\n      </v-row>\r\n\r\n      <v-row>\r\n        <v-col>\r\n          <label class=\"font-weight-bold mt-n2\">LTE TYPE :</label>\r\n          <v-text-field\r\n            class=\"mr-2 mb-n2\"\r\n            dense\r\n            filled\r\n            shaped\r\n            ref=\"lte_type\"\r\n            v-model=\"lte_type\"\r\n            :rules=\"lte_type_rule\"\r\n            label=\"* LTE type (e.g. AMT or QTEL)\"\r\n            required\r\n          ></v-text-field>\r\n        </v-col>\r\n        <v-col>\r\n          </v-col>\r\n        </v-row>\r\n      </v-card>\r\n      <br /><br />\r\n      \r\n      <!-- mission START -->\r\n\r\n      <v-card class=\"pa-7\" outlined shaped elevation=\"3\">\r\n        <v-row>\r\n        <h5 class=\"cardtitle font-weight-black\">Mission Information</h5>\r\n        <br><br>\r\n          <v-col>\r\n            <p class=\"font-weight-bold\">MISSION NAME :</p>\r\n\r\n            <v-select\r\n          :items=\"missions\"\r\n          ref=\"mission_name\"\r\n          v-model=\"mission_name\"\r\n          :rules=\"mission_name_rule\"\r\n          filled\r\n          shaped\r\n          label=\"Select mission\"\r\n          required\r\n        ></v-select>\r\n       \r\n       <!--\r\n          <v-text-field\r\n          class=\"mr-2 mb-n2\"\r\n          dense\r\n          filled\r\n          shaped\r\n          ref=\"mission_name\"\r\n          v-model=\"mission_name\"\r\n          :rules=\"mission_name_rule\"\r\n          label=\"* mission name (e.g. ??)\"\r\n          required\r\n        ></v-text-field>\r\n        -->\r\n\r\n      </v-col>\r\n \r\n\r\n      <label v-if=\"mission_name !== null\" class=\"font-weight-bold mt-n2\"\r\n        >mission container:</label\r\n      >\r\n      <v-row v-if=\"mission_name !== null\" no-gutters>\r\n        <v-col>\r\n          <v-text-field\r\n            class=\"mr-2 mb-n2\"\r\n            dense\r\n            filled\r\n            shaped\r\n            ref=\"mission_container\"\r\n            v-model=\"mission_container\"\r\n            :rules=\"mission_container_rule\"\r\n            label=\"* mission container\"\r\n            required\r\n          ></v-text-field>\r\n        </v-col>\r\n      </v-row>\r\n\r\n      <label v-if=\"mission_name !== null\" class=\"font-weight-bold mt-n2\"\r\n        >mission sub_container:</label\r\n      >\r\n      <v-row v-if=\"mission_name !== null\" no-gutters>\r\n        <v-col>\r\n          <v-text-field\r\n            class=\"mr-2 mb-n2\"\r\n            dense\r\n            filled\r\n            shaped\r\n            ref=\"mission_subcontainer\"\r\n            v-model=\"mission_subcontainer\"\r\n            :rules=\"mission_subcontainer_rule\"\r\n            label=\"* mission sub_container\"\r\n            required\r\n          ></v-text-field>\r\n        </v-col>\r\n      </v-row>\r\n\r\n      <label v-if=\"mission_name !== null\" class=\"font-weight-bold mt-n2\"\r\n        >mission URL:</label\r\n      >\r\n      <v-row v-if=\"mission_name !== null\" no-gutters>\r\n        <v-col>\r\n          <v-text-field\r\n            class=\"mr-2 mb-n2\"\r\n            dense\r\n            filled\r\n            shaped\r\n            ref=\"mission_git\"\r\n            v-model=\"mission_git\"\r\n            :rules=\"mission_git_rule\"\r\n            label=\"* mission git URL\"\r\n            required\r\n            persistent-hint\r\n          ></v-text-field>\r\n        </v-col>\r\n      </v-row>\r\n\r\n\r\n\r\n      <v-row>\r\n        <v-col></v-col>\r\n        <v-col cols=\"3\">\r\n          <v-btn\r\n            class=\"mt-2\"\r\n            @click=\"AddMission\"\r\n            elevation=\"1\"\r\n            color=\"#64B5F6\"\r\n            :disabled=\"mission_name === null\"\r\n            block\r\n            outlined\r\n            raised\r\n            rounded\r\n          >\r\n          Add Mission\r\n          </v-btn>\r\n        </v-col>\r\n        <v-col cols=\"3\">\r\n          <v-btn\r\n            class=\"mt-2\"\r\n            @click=\"DeleteMission\"\r\n            elevation=\"1\"\r\n            color=\"#64B5F6\"\r\n            :disabled=\"!add_mission_flag\"\r\n            block\r\n            outlined\r\n            raised\r\n            rounded\r\n            v-on:click=\"ae_form\"\r\n          >\r\n           Delete Mission\r\n          </v-btn>\r\n        </v-col>\r\n        <v-col></v-col>\r\n      </v-row>\r\n\r\n\r\n      <v-row class=\"mt-2\">\r\n        <v-col no-gutters>\r\n          <v-col>\r\n            <hr color=\"dimgrey\" />\r\n            <label v-if=\"add_mission_flag\" class=\"font-weight-bold\"\r\n              >Mission List</label\r\n            >\r\n          </v-col>\r\n          <v-col class=\"mt-n4\">\r\n            <v-data-table\r\n              v-if=\"add_mission_flag\"\r\n              v-model=\"mission_selected\"\r\n              :headers=\"mission_col\"\r\n              :items=\"mission_data\"\r\n              item-key=\"mission_name\"\r\n              class=\"elevation-1\"\r\n              hide-default-footer\r\n            >\r\n              <template v-slot:item=\"{ item }\">\r\n                <tr\r\n                  :class=\"\r\n                    mission_selected.indexOf(item.mission_name) > -1\r\n                      ? 'grey'\r\n                      : ''\r\n                  \"\r\n                  @click=\"rowC